      &lt;span class=&quot;subtitle&quot;&gt;&lt;p&gt;&lt;code&gt;openssl&lt;/code&gt; &lt;code&gt;X509VerifyParamRef::set_host&lt;/code&gt; buffer over-read&lt;/p&gt;
&lt;/span&gt;
    &lt;/header&gt;

    

    &lt;dl&gt;
      &lt;dt id=&quot;reported&quot;&gt;Reported&lt;/dt&gt;
      &lt;dd&gt;
        &lt;time datetime=&quot;2023-06-20&quot;&gt;
          June 20, 2023
        &lt;/time&gt;
      &lt;/dd&gt;

      &lt;dt id=&quot;issued&quot;&gt;Issued&lt;/dt&gt;
      &lt;dd&gt;
        &lt;time datetime=&quot;2023-06-20&quot;&gt;
          June 20, 2023
        &lt;/time&gt;
        
        &lt;time datetime=&quot;2023-07-08&quot;&gt;
          (last modified: July  8, 2023)
        &lt;/time&gt;
        
      &lt;/dd&gt;

      &lt;dt id=&quot;package&quot;&gt;Package&lt;/dt&gt;
      &lt;dd&gt;
        
        
        &lt;a href=&quot;/packages/openssl.html&quot;&gt;openssl&lt;/a&gt;
          (&lt;a href=&quot;https://crates.io/crates/openssl&quot;&gt;crates.io&lt;/a&gt;)
        
        
